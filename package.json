{
  "name": "obsidian-metatable",
  "version": "0.11.0",
  "description": "An Obsidian plugin to render the full frontmatter as a table.",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js --environment BUILD:production",
    "dist": "mkdir -p dist && cp main.js versions.json manifest.json dist/",
    "test_vault": "cp main.js versions.json manifest.json test_vault/.obsidian/plugins/obsidian-metatable",
    "test": "jest"
  },
  "keywords": [
    "obsidian"
  ],
  "author": "Arnau Siches",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "jest": "^27.3.1",
    "obsidian": "https://github.com/obsidianmd/obsidian-api/tarball/master",
    "rollup": "^2.48.0",
    "rollup-plugin-import-css": "^3.0.2",
    "ts-jest": "^27.0.7",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  }
}
